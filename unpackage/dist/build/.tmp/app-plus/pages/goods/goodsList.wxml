<view><view class="header"><block wx:for="{{filterByList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['handleTarget',[index]]]]]}}" class="{{['target',(item.selected)?'on':'']}}" bindtap="__e">{{''+item.text+''}}</view></block></view><view class="place"></view><view class="goods-list"><view class="product-list"><block wx:for="{{goodsList}}" wx:for-item="product" wx:for-index="__i0__" wx:key="goods_id"><view class="product"><image src="{{product.img}}" mode="widthFix"></image><view class="name">{{''+product.name+''}}</view><view class="info"><view class="price">{{'$'+product.price+''}}</view><view class="slogan">{{''+product.slogan+'已购买'}}</view></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></view>